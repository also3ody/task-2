#include <iostream>

using namespace std;

int main ()
{
    int num1,num2;
    cout << "please enter the first number : ";
    cin >> num1;
    cout << "please enter the second number : ";
    cin >> num2;
    if (num1>num2)
    {
        cout << "The first number is greater";
    }
    else if (num1<num2)
    {
        cout << "The second number is greater";
    }
    else
    {
        cout << "Both numbers are equal";
    }
}
